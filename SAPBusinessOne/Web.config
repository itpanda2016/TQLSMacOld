<configuration>
  <system.web>
    <!--Session配置-->
    <sessionState timeout="30"/>
    <customErrors mode="On"/>
    <compilation debug="true"/>
  </system.web>
  <appSettings>
    <add key="logPath" value="~/logs/"/>
  </appSettings>
</configuration>